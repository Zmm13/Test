<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <declare-styleable name="DragImageView">
        <attr name="side_padding" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="MyCheckBar">
        <attr name="bg_color_default" format="color"/>
        <attr name="bg_color_checked" format="color"/>
        <attr name="circle_color_default" format="color"/>
        <attr name="circle_color_checked" format="color"/>
        <attr name="isCheck" format="boolean"/>
    </declare-styleable>
</resources>
